#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>
#include <math.h> 

long long solution(long long n) {
    long long answer = 0;// long 은 정수형 
    long long x = 0 ;
    x=sqrt(n);// x 는 결국 정수형만 남음
    if(x==sqrt(n))
    { answer = pow(x+1,2);}
    else
    { answer = -1;}    
    return answer;
}
